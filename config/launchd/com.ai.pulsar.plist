<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>com.ai.pulsar</string>
  <key>ProgramArguments</key>
  <array>
    <string>/opt/pulsar/bin/pulsar-daemon</string>
    <string>start</string>
    <string>standalone</string>
  </array>
  <key>RunAtLoad</key>
  <true/>
  <key>KeepAlive</key>
  <true/>
  <key>StandardOutPath</key>
  <string>$HOME/Library/Logs/pulsar.out</string>
  <key>StandardErrorPath</key>
  <string>$HOME/Library/Logs/pulsar.err</string>
</dict>
</plist>
